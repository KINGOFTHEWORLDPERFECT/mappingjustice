html
    head
        title The Map
        link(rel='stylesheet', href='https://unpkg.com/leaflet@1.0.0-rc.3/dist/leaflet.css')
    body
        #mapid(style='height:100%;')
        script(src='https://unpkg.com/leaflet@1.0.0-rc.3/dist/leaflet.js')
        script.
            var mymap = new L.map('mapid').setView([33.777279, -84.397287], 13);
            var osmUrl = 'https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png';
            var osmAttrib='Map data Â© <a href="http://openstreetmap.org">OpenStreetMap</a> contributors';
            var osm = new L.TileLayer(osmUrl, {attribution: osmAttrib}).addTo(mymap); 
            
            L.marker([33.777279, -84.397287]).addTo(mymap).bindPopup('College of Computing').openPopup();
